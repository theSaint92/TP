<Window x:Class="GUI.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GUI"
        xmlns:vm="clr-namespace:GUI.ViewModel"
        mc:Ignorable="d"
        Title="Casino" Height="600" Width="600">
    <Window.DataContext>
        <vm:ViewModel/>
    </Window.DataContext>

    <StackPanel>
        <Label Content="Participations:"></Label>
        <ListBox Name="Some"></ListBox>
        <Button x:Name="BenjaminButton" Content="Benjamin Button" Command="{Binding AddClientCommand}"></Button>
    </StackPanel>
</Window>
